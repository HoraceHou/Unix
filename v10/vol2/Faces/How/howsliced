x {
	int a, dy;
	array ddy[X];
	for (dy = 1; dy < X; dy++)
		ddy[dy] = ddy[dy-1]+rand()%4-2
	for (dy = 0; dy < Y; dy++)
	{	a = (dy/51)%4
		for (x = 0; x < X; x++)
		{	y = clamp(ddy[x]+dy-sin(2*x)/(x+50)) 
			if (a == 0)
			new[x,y]=$1[x,y]
			if (a == 1)
			new[x,y]=$2[x,y]
			if (a == 2)
			new[x,y]=$5[x,y]
			if (a == 3)
			new[x,y]=$4[x,y]
		}
	}	
}
